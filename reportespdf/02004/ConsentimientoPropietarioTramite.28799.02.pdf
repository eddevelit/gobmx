CREATE OR REPLACE TRIGGER "DGGFS_SNGF_EV2".CLAVEASEN
BEFORE INSERT ON ASENTAMIENTO 
REFERENCING NEW AS NEW FOR EACH ROW 
DECLARE valorSecuencia NUMBER := 0;
BEGIN
SELECT CLAVEASEN.NEXTVAL INTO valorSecuencia FROM DUAL;
:NEW.CVE_ASEN := valorSecuencia;
END;

CREATE SEQUENCE  "DGGFS_SNGF_EV2"."CLAVEASEN"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOPARTITION ;